<!--pages/user/user.wxml-->
<view class="container">
  <!--轮播图-->
  <view class='main-lunbo'>
    <swiper indicator-dots='true' autoplay='true' interval='4000' class='lunbo'>
      <swiper-item><image src='../../images/1.jpg'></image></swiper-item>
      <swiper-item><image src='../../images/3.jpg'></image></swiper-item>
    </swiper>
  </view>
  <!--功能导航-->
  <view class="main-core">
    <block wx:for="{{core}}" wx:key="id">
      <navigator class="main-core-item" url="{{'/pages/function/'+item.id+'/'+item.id}}">
        <image class="core-item-icon" src="/assets/core/{{item.id}}.png"></image>
        <text class="core-item-name">{{item.name}}</text>
      </navigator>
    </block>
  </view>
  <!--天气预报-->
  <view class='tianqi'>
    <view></view>
  </view>
</view>